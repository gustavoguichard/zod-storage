{
  "name": "zod-storage",
  "version": "0.0.0",
  "description": "Type-safe local and session storage",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Gustavo Guichard <@gugaguichard>",
